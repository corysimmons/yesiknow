    "serve-react": "yarn --cwd react-app serve",
    "electron": "electron ."
  },
  "dependencies": {
    "electron": "^15.5.5",
    "is-electron": "^2.2.0",
    "npm-run-all": "^4.1.5"
  },
  "main": "main.js"
}
