{
  "scripts": {
    "start": "NODE_ENV=production run-s build-react serve",
    "dev": "NODE_ENV=development run-p dev-react electron",
    "serve": "run-p serve-react electron",
    "dev-react": "BROWSER=none yarn --cwd react-app start",
